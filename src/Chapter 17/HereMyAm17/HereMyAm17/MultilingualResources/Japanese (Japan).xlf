        <trans-unit id="resources\app_description" translate="no" xml:space="preserve">
          <source>Here My Am! (17) has been globalized (removing locale-specific assumptions) and localized into several languages by moving language-specific strings into resource files and providing localized images.</source>
          <target state="signed-off">Here My Am! (17) has been globalized (removing locale-specific assumptions) and localized into several languages by moving language-specific strings into resource files and providing localized images.</target>
        </trans-unit>
        <trans-unit id="resources\app_title" translate="no" xml:space="preserve">
          <source>Here My Am! (17)</source>
          <target state="signed-off">Here My Am! (17)</target>
        </trans-unit>
        <trans-unit id="resources\appbar_label1" translate="yes" xml:space="preserve">
          <source>Load picture</source>
          <target state="signed-off">写真の読込</target>
        </trans-unit>
        <trans-unit id="resources\appbar_label2" translate="yes" xml:space="preserve">
          <source>Recent pictures</source>
          <target state="signed-off" state-qualifier="mt-suggestion">最近の写真</target>
        </trans-unit>
        <trans-unit id="resources\appbar_label3" translate="yes" xml:space="preserve">
          <source>Refresh location</source>
          <target state="signed-off">場所の更新</target>
        </trans-unit>
        <trans-unit id="resources\appbar_tooltip1" translate="yes" xml:space="preserve">
          <source>Load a picture through the file picker</source>
          <target state="signed-off">ファイル ピッカーを使って画像を読み込み</target>
        </trans-unit>
        <trans-unit id="resources\appbar_tooltip2" translate="yes" xml:space="preserve">
          <source>Browse recent pictures taken in the app</source>
          <target state="signed-off">アプリで撮影した最近の写真を参照</target>
        </trans-unit>
        <trans-unit id="resources\appbar_tooltip3" translate="yes" xml:space="preserve">
          <source>Refresh your location with current GPS coordinates</source>
          <target state="signed-off">あなたの現在の場所でGPS座標を更新します。</target>
        </trans-unit>
        <trans-unit id="resources\aria_about" translate="yes" xml:space="preserve">
